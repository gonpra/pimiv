<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object class="GtkBox" id="entry_box">
        <property name="orientation">vertical</property>
        <property name="spacing">5</property>

        <child>
            <object class="GtkEntry" id="username_entry">
                <property name="placeholder-text">Nome de usu√°rio</property>
            </object>
        </child>

        <child>
            <object class="GtkEntry" id="password_entry">
                <property name="placeholder-text">Senha</property>
                <property name="visibility">false</property>
            </object>
        </child>

        <child>
            <object class="GtkEntry" id="confirm_password_entry">
                <property name="placeholder-text">Confirmar senha</property>
                <property name="visibility">false</property>
            </object>
        </child>

        <child>
            <object class="GtkBox" id="button_box">
                <property name="orientation">horizontal</property>
                <property name="spacing">3</property>

                <child>
                    <object class="GtkButton" id="register_button">
                        <property name="label">Registrar</property>
                        <!--                        <signal name="clicked" handler="on_button_user_register_clicked"/>-->
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
