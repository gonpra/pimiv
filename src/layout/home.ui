    <property name="child">
      <object class="GtkLabel">
        <binding name="label">
          <lookup name="name" type="TableCompany">
            <lookup name="item">GtkListItem</lookup>
          </lookup>
        </binding>
      </object>
    </property>
  </template>
</interface>
]]></property>
                            </object>
